			<script src="http://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
			<script src="http://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
		<![endif]-->

	{{! Google Analytics }}


	{{! Ghost Head Outputs }}
		{{ghost_head}}

</head>

<body class="{{body_class}} clearfix" itemscope itemtype="http://schema.org/WebPage">
	{{! Facebook API }}
		<div id="fb-root"></div>
		<script>(function(d, s, id) {
			var js, fjs = d.getElementsByTagName(s)[0];
			if (d.getElementById(id)) return;
			js = d.createElement(s); js.id = id;
			js.src = "//connect.facebook.net/en_GB/all.js#xfbml=1";
			fjs.parentNode.insertBefore(js, fjs);
		}(document, 'script', 'facebook-jssdk'));</script>

	{{! Header }}
		<header>
			<div class="clearfix">
				<div id="header" class="one-whole-percent bm-remove text-center" {{#if @blog.cover}}style="background-image: url({{@blog.cover}});"{{/if}}>
					<div id="logo-description" class="center-vert">
						<a href="{{@blog.url}}" target="_self">
							{{#if @blog.logo}}
								<p id="logo-img" class="bm-small"><img src="{{@blog.logo}}" alt="{{@blog.title}}" /></p>
							{{else}}
								<p id="logo-text" class="bm-small">{{@blog.title}}</p>
							{{/if}}

							<p id="blog-description" class="bm-remove">{{@blog.description}}</p>
						</a>
					</div>

					{{!-- <div id="scroll-to-content" class="cursor-pointer"><i class="fa fa-angle-down"></i></div> --}}

					<nav>
						<div class="clearfix">
							<div id="no-touch-navigation" class="one-whole-percent bm-remove text-uppercase">
								<div class="one-half-percent desktop-tablet bm-remove text-center">
									<ul class="unstyled inline bm-remove links">
										<li><a href="/" target="_self">Home</a></li>
										<li><a href="/about-us" target="_self">Who We Are</a></li>
										<li><a href="/plans" target="_self">Browse Plans</a></li>
										<li><a href="/neighborhoods" target="_self">Find a Neighborhood</a></li>
										<li><a href="/contact-us" target="_self">Contact Us</a></li>
									</ul>
								</div>

								<div class="one-half-percent desktop-tablet bm-remove last">
									<div class="float-right">
										<ul class="unstyled inline bm-remove social-networks">
											<li><a href="https://www.facebook.com/gchomeskc" target="_blank" class="tip-t-fade" data-tooltip="Facebook"><i class="fa fa-facebook fa-fw"></i></a></li>
											{{!-- <li><a href="#" target="_self" class="tip-t-fade" data-tooltip="Twitter"><i class="fa fa-twitter fa-fw"></i></a></li> --}}
											{{!-- <li><a href="#" target="_self" class="tip-t-fade" data-tooltip="Google+"><i class="fa fa-google-plus fa-fw"></i></a></li> --}}
											<li><a href="http://www.pinterest.com/eaglevision20/" target="_blank" class="tip-t-fade" data-tooltip="Pinterest"><i class="fa fa-pinterest fa-fw"></i></a></li>
											{{!-- <li><a href="#" target="_self" class="tip-t-fade" data-tooltip="Instagram"><i class="fa fa-instagram fa-fw"></i></a></li>
											<li><a href="#" target="_self" class="tip-t-fade" data-tooltip="Flickr"><i class="fa fa-flickr fa-fw"></i></a></li>
											<li><a href="#" target="_self" class="tip-t-fade" data-tooltip="RSS"><i class="fa fa-rss fa-fw"></i></a></li> --}}
										</ul>
									</div>
								</div>
							</div>
						</div>
					</nav>
				</div>
			</div>
		</header>

		<nav>
			<div id="touch-navigation" class="text-center">
				<a href="#" target="_self" id="navigation-toggle"><i class="fa fa-bars fa-fw"></i></a>

				<div id="menu-slide">
					<hr class="bm-small" />

					<ul class="unstyled bm-remove links"></ul>

					<hr class="tm-small bm-small" />

					<ul class="unstyled inline inline-block bm-remove social-networks">
					</ul>
				</div>
			</div>
		</nav>

	{{! No JavaScript }}
		<noscript>
			<div class="clearfix">
				<div class="container bm-larger tm-larger text-center">
					<div id="no-script">
						<p class="bm-smaller"><strong>JavaScript Disabled</strong></p>
						<p class="bm-smaller">Certain features of this site may not function correctly without JavaScript enabled</p>
						<p class="bm-remove"><a href="http://enable-javascript.com/" target="_blank">Find out how to enable JavaScript in your browser</a></p>
					</div>
				</div>
			</div>

			<hr class="bm-remove separator" />
		</noscript>

	{{! Content }}
		{{{body}}}

	{{! Footer }}
		<footer>
			<div id="footer" class="one-whole-percent bm-remove text-center animate slow-mo" data-anim-type="fadeIn">
				<div class="scroll-to-top cursor-pointer tip-t-fade" data-tooltip="Scroll To Top"><i class="fa fa-chevron-circle-up fa-fw"></i></div>

				<div class="container bm-remove">
					Copyright &copy; <a href="/" target="_self">{{@blog.title}}</a> {{date format="YYYY"}}. Proudly published with <a href="http://ghost.org" target="_blank">Ghost</a>.
				</div>
			</div>
		</footer>

